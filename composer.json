{
    "name": "zeix/craft-boarding",
    "description": "An onboarding plugin for the Control Panel.",
    "type": "craft-plugin",
    "version": "1.1.4",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "boarding",
        "onboarding",
        "tours"
    ],
    "support": {
        "docs": "https://github.com/zeixcom/craft-boarding/blob/main/README.md",
        "issues": "https://github.com/zeixcom/craft-boarding/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Zeix AG",
            "homepage": "https://zeix.com"
        }
    ],
    "require": {
        "craftcms/cms": "^5.0.0",
        "php": "^8.0.2"
    },
    "require-dev": {
        "codeception/codeception": "^5.0",
        "codeception/module-yii2": "^1.1",
        "codeception/module-asserts": "^3.0",
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main",
        "craftcms/rector": "dev-main",
        "phpstan/phpstan": "^1.10.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "zeix\\boarding\\": "src/"
        }
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan analyse --memory-limit=256M",
        "rector": "rector process --ansi",
        "check": [
            "@check-cs",
            "@phpstan",
            "@test"
        ]
    },
    "extra": {
        "name": "Boarding",
        "handle": "boarding",
        "developer": "Zeix",
        "developerUrl": "https://zeix.com",
        "documentationUrl": "https://github.com/zeixcom/craft-boarding/blob/main/README.md",
        "changelogUrl": "https://github.com/zeixcom/craft-boarding/blob/main/CHANGELOG.md",
        "class": "zeix\\boarding\\Boarding"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
